    "test": "mocha 'test/**/*.spec.{js,jsx}'",
    "test:watch": "npm run test -- --watch --watch-files src,test",
    "fmt:base": "prettier '{src,test}/**/*.{js,jsx}'",
    "fmt": "npm run fmt:base -- --write",
    "fmt:check": "npm run fmt:base -- --check"
  },
