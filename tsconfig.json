{
  "compilerOptions": {
    "typeRoots": ["./types", "./node_modules/@types"],
    "baseUrl": ".",
    "moduleResolution": "nodenext",
    "checkJs": true,
    "explainFiles": false,
    "target": "esnext",
    "module": "nodenext",
    "strictNullChecks": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "types"]
}
